{
  "name": "PTS UI (Node 20)",
  "image": "mcr.microsoft.com/devcontainers/javascript-node:20",
  "workspaceFolder": "/workspaces/gammon-pts-frontend/${localWorkspaceFolderBasename}",
  "remoteUser": "node",
  "containerEnv": {
  "NG_CLI_ANALYTICS": "false"
  },
  "features": {},
  "mounts": [
    "source=/home/orca/.gitconfig,target=/home/node/.gitconfig,type=bind,consistency=cached",
    "source=/home/orca/.gnupg,target=/home/node/.gnupg,type=bind,consistency=cached",
    "source=/home/orca/.azure,target=/home/node/.azure,type=bind,consistency=cached",
    "source=/home/orca/.bashrc,target=/home/node/.bashrc,type=bind,consistency=cached"
  ],
  "postCreateCommand": "npm install && npm install -g @angular/cli",
  "customizations": {
    "vscode": {
      "extensions": [
        "Angular.ng-template",
        "esbenp.prettier-vscode",
        "kisstkondoros.vscode-codemetrics",
        "nhoizey.gremlins",
        "visualstudioexptteam.intellicode-api-usage-examples",
        "eamodio.gitlens",
        "mhutchie.git-graph"
      ]
    }
  }
}
